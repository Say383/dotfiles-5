# terminal
cmd - return: /Applications/Alacritty.app/Contents/MacOS/alacritty

# editor
fn + cmd - e: emacsclient -c -a ""
fn + cmd - i: /Applications/Firefox.app/Contents/MacOS/firefox
fn + cmd - f: open .

# music
fn + cmd - f1: mpc toggle
fn + cmd - f2: mpc play
fn + cmd - f3: mpc pause
fn + cmd - f4: mpc prev
fn + cmd - f5: mpc next
fn + cmd - f6: mpc volume -10
fn + cmd - f7: mpc volume +10
